    "oni": "node oni",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --ext .js",
      "git add ."
    ]
  },
  "author": "onilab",
  "license": "ISC",
  "homepage": "no",
  "dependencies": {
    "@apollo/client": "^3.4.17",
    "@pwa-concept/build": "^0.0.0",
    "@pwa-concept/eslint-config": "^0.0.0",
    "@svg-maps/world": "^1.0.1",
    "@svgr/webpack": "^5.5.0",
    "apollo-link-queue": "^3.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "core-js": "^3.19.1",
    "graphql": "^16.0.1",
    "html-react-parser": "^1.4.0",
    "js-cookie": "^3.0.1",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.19.5",
    "react-inner-image-zoom": "^2.1.0",
    "react-intersection-observer": "^8.32.2",
    "react-range": "^1.8.9",
    "react-router-dom": "^5.2.0",
    "react-spring-bottom-sheet": "^3.4.0",
    "react-transition-group": "^4.4.2",
    "react-zoom-pan-pinch": "^2.1.3",
    "scroll-lock": "^2.1.5",
    "swiper": "^7.2.0"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "cross-env": "^7.0.3",
    "get-graphql-schema": "^2.1.2",
    "husky": "^6.0.0",
    "inquirer-directory": "^2.2.0",
    "lint-staged": "^11.2.6",
    "plop": "^2.7.6"
  },
  "eslintConfig": {
    "extends": "@pwa-concept/eslint-config"
  }
