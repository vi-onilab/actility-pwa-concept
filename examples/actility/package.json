{
  "name": "actility",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "pwa-concept start",
    "build": "pwa-concept build",
    "analyze": "pwa-concept analyze",
    "lint": "eslint src --ext .js",
    "lint:fix": "npm run lint --fix",
    "schema:pull": "get-graphql-schema $(grep SCHEMA_PULL_URL .env | cut -d '=' -f2) > ./src/schemas/schema.graphql",
