{
  "name": "actility",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "serve": "cross-env NODE_ENV=development AUTO_HMR=true webpack serve --mode development",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "build:analyze": "cross-env NODE_ENV=production ANALYZE=true webpack --mode production",
    "lint": "eslint src --ext .js",
    "lint:fix": "npm run lint --fix",
    "schema:pull": "get-graphql-schema $(grep SCHEMA_PULL_URL .env | cut -d '=' -f2) > ./src/schemas/schema.graphql",
    "prepare": "husky install",
    "oni": "node oni",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --ext .js",
      "git add ."
    ]
  },
  "author": "onilab",
  "license": "ISC",
  "homepage": "no",
